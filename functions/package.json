  "license": "MIT",
  "engines": {
    "node": "22"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "check": "biome check ./src",
    "fix": "biome check --write ./src",
    "genkit": "npm run build && genkit start -o --attach http://localhost:3100 --port 4001",
    "emulator": "npm run build && GENKIT_ENV=dev firebase emulators:start --inspect-functions",
    "deploy": "npm run build && firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@genkit-ai/ai": "^0.5.4",
    "@genkit-ai/core": "^0.5.4",
    "@genkit-ai/dotprompt": "^0.5.10",
    "@genkit-ai/firebase": "^0.5.4",
    "@genkit-ai/flow": "^0.5.4",
    "@slack/bolt": "^3.19.0",
    "csv-parse": "^5.5.6",
    "express": "^4.19.2",
    "firebase-admin": "^12.3.1",
    "firebase-functions": "^5.0.1",
    "genkitx-openai": "^0.10.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
